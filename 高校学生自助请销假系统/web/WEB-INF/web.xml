<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Ass_man</servlet-name>
        <servlet-class>Ass_man</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Adm_man</servlet-name>
        <servlet-class>Adm_man</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tea_man</servlet-name>
        <servlet-class>Tea_man</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Stu_man</servlet-name>
        <servlet-class>Stu_man</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>Stu_man</servlet-name>
        <url-pattern>/stu</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>Adm_man</servlet-name>
        <url-pattern>/Adm</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>Ass_man</servlet-name>
        <url-pattern>/ass</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>Tea_man</servlet-name>
        <url-pattern>/tea</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login.html</welcome-file>
    </welcome-file-list>
</web-app>
